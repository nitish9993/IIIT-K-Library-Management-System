            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="email" layoutX="31.0" layoutY="64.0" prefHeight="41.0" prefWidth="408.0" promptText="Username" style="-fx-background-color: white;">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="31.0" layoutY="124.0" prefHeight="41.0" prefWidth="408.0" promptText="Password" style="-fx-background-color: white;">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="repassword" layoutX="32.0" layoutY="181.0" prefHeight="41.0" prefWidth="408.0" promptText="Retype Password" style="-fx-background-color: white;">
