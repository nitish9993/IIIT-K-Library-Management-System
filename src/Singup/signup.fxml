<AnchorPane id="AnchorPane" fx:id="anchorpane" prefHeight="431.0" prefWidth="468.0" style="-fx-background-color: #1d71f7;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Singup.signupController">
    <stylesheets>
        <URL value="@signup.css" />
    </stylesheets>
   <children>
      <Label layoutX="118.0" layoutY="14.0" prefHeight="34.0" prefWidth="232.0" text="Register For New Member " textFill="#fbfcf7">
