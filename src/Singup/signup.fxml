            <Font size="18.0" />
         </font>
      </TextField>
      <JFXButton fx:id="register" buttonType="RAISED" layoutX="185.0" layoutY="303.0" onAction="#onRegister" prefHeight="41.0" prefWidth="101.0" ripplerFill="#131fcd" style="-fx-background-color: white; -fx-background-radius: 25;" text="Register" textFill="#0a0a0a">
